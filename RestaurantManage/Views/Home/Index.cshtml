			        </div>
			        @if (foods != null)
			        {
				        <div class="row">
					        <div class="col-lg-6">
						        <div class="discover_item_inner">
							        @for (int i = 0; i < 4; i++)
							        {
								        <div class="discover_item">
									        @if (i < foods.Count && foods[i] != null)
									        {
										        <h4>@foods[i].Name</h4>
										        <p>@foods[i].Name <span>@foods[i].Price đ</span></p>
									        }
								        </div>
							        }
						        </div>
					        </div>
					        <div class="col-lg-6">
						        <div class="discover_item_inner">
							        @for (int i = 4; i < 8; i++)
							        {
								        <div class="discover_item">
									        @if (i < foods.Count && foods[i] != null)
									        {
										        <h4>@foods[i].Name</h4>
										        <p>@foods[i].Name <span>@foods[i].Price đ</span></p>
									        }
								        </div>
							        }
						        </div>
					        </div>
				        </div>
			        }
        		</div>
        	</div>
        </section>
